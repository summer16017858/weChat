/**app.wxss**/
@import "./components/index.wxss";
@import "./weui.wxss";
@font-face {
  font-family: 'iconfont';  /* project id 425285 */
  src: url('//at.alicdn.com/t/font_425285_iphk8jcp5zgaxlxr.eot');
  src: url('//at.alicdn.com/t/font_425285_iphk8jcp5zgaxlxr.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_425285_iphk8jcp5zgaxlxr.woff') format('woff'),
  url('//at.alicdn.com/t/font_425285_iphk8jcp5zgaxlxr.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_425285_iphk8jcp5zgaxlxr.svg#iconfont') format('svg');
}
.iconfont{
  font-family:"iconfont";
  font-size:14px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
  margin-left:10px;
}
.arrow-down:after{
  content: "\e600"
}
.weui-cells{font-size:14px;}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.weui-label{
  width:80px;
  height:2.58823529em;
  min-height:2.58823529em;
  line-height:2.58823529em;

}
.weui-uploader{
  display: flex;
  flex-direction: row;
}
.weui-cells{
  margin-top:0;
}
.weui-cells:after{
  border-bottom:none
}
.weui-check__label{
  padding-left:0;
}